    1 REM NOTE: Basic language type forced to be:
    2 REM       trs2 - TRS-80 Level II BASIC tokenized file
   10 CLS
   20 PRINT@283,"3-D PLOT"
   60 FORQZ=1TO1000
   70 NEXTQZ
   80 CLS
   90 PRINT"DO YOU HAVE A PRINTER";
  100 INPUTA$:IFLEFT$(A$,1)="Y"THEN120
  110 GOTO160
  120 Q=1
  130 PRINT"TURN ON YOUR PRINTER AND HIT ANY KEY"
  140 A$=INKEY$:IFA$=""THEN140
  150 GOTO190
  160 Q=0
  170 GOTO190
  180 PRINT"THERE ARE ONLY SIX FUNCTIONS, ONE THROUGH SIX INCLUSIVE"
  190 CLS:PRINT"WHICH FUNCTION WOULD YOU LIKE TO HAVE ME PRINT <1-6>"
  200 INPUTA:IFA>6ORA<1THEN180
  210 CLS
  220 FORX=-30TO30STEP1.5
  230 L=0
  240 Y1=5*INT(SQR(900-X*X)/5)
  250 FORY=Y1TO-Y1STEP-5
  260 P=SQR(X*X+Y*Y)
  270 GOSUB390
  280 IFZ<=LTHEN330
  290 L=Z
  300 IFQ=0THEN320
  310 LPRINTTAB(Z);"*";
  320 PRINTTAB(Z);"*";
  330 NEXTY
  340 IFQ=0THEN360
  350 LPRINT
  360 PRINT
  370 NEXTX
  380 GOTO520
  390 ONAGOTO400,420,440,460,480,500
  400 Z=INT(25+(30*CSNG(-P*P/100))-.7*Y)
  410 GOTO510
  420 Z=INT(25+(SQR(900.01-P*P)*.9-2)-.7*Y)
  430 GOTO510
  440 Z=INT(25+(30*(COS(P/16)^2))-.7*Y)
  450 GOTO510
  460 Z=INT(25+(30-30*SIN(P/19))-.7*Y)
  470 GOTO510
  480 Z=INT(25+(30*CSNG(-COS(P/16))-30)-.7*Y)
  490 GOTO510
  500 Z=INT(25+(30*SIN(P/10))-.7*Y)
  510 RETURN
  520 PRINT"WOULD YOU LIKE ME TO PRINT ANOTHER FUNCTION";
  530 INPUTB$:IFLEFT$(B$,1)="Y"THEN190
  540 END
